#:lang cm

-- filters so only non-negative floats are present,
-- then converts all to ints
@ let vals :=
    (appl filter 
        ((\x -> not (neg?:x)),
        (5.7,-0.0,6.8,-5.8;);)) 
    in appl map ((\x -> int x),vals;)//

-- returns the max of each sub pair (7, 9, 7;)
@ appl map
    ((\x -> appl max (`x,~x;)),
    ((4,7),(5,9),(7,3););)//
